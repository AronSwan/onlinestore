# 开发环境配置文件

# 数据库配置
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=secure_postgres_password
DB_DATABASE=shopping_db

# JWT 配置
JWT_SECRET=super_secret_jwt_key_make_it_long_and_random_32_chars_min

# OpenObserve 配置（设置为本地地址或禁用）
OPENOBSERVE_URL=http://localhost:5080
OPENOBSERVE_USERNAME=admin@example.com
OPENOBSERVE_PASSWORD=ComplexPass#123

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379

# 应用配置
NODE_ENV=development
PORT=3000
API_PREFIX=api

# CORS 配置
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://127.0.0.1:3000

# 日志配置
LOG_LEVEL=debug

# 加密配置（64字符长度）
ENCRYPTION_KEY=a1b2c3d4e5f6789012345678901234567890abcdef1234567890abcdef123456