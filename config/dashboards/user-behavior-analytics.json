        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        }
      },
      {
        "id": "geographic-distribution",
        "title": "地理分布",
        "type": "worldmap",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 48
        },
        "targets": [
          {
            "refId": "A",
            "query": "SELECT geoLocation.country as country, COUNT(*) as sessions FROM user-behavior WHERE eventType = 'page_view' AND timestamp >= now() - INTERVAL '24 hours' GROUP BY geoLocation.country ORDER BY sessions DESC",
            "datasource": "openobserve",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": []
          },
          "overrides": []
        },
