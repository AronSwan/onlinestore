                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              }
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": []
        }
      },
      {
        "id": "form-interactions",
        "title": "表单交互分析",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 40
        },
        "targets": [
          {
            "refId": "A",
            "query": "SELECT form.id, form.action, COUNT(*) as submissions, COUNT(DISTINCT sessionId) as unique_sessions, AVG(form.fieldCount) as avg_fields FROM user-behavior WHERE eventType = 'form_submit' AND timestamp >= now() - INTERVAL '24 hours' GROUP BY form.id, form.action ORDER BY submissions DESC",
            "datasource": "openobserve",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "submissions"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {
                        "color": "green",
                        "value": null
                      },
                      {
                        "color": "yellow",
                        "value": 10
                      },
                      {
                        "color": "red",
                        "value": 50
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true
        }
      },
      {
        "id": "performance-metrics",
        "title": "性能指标",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 48
        },
        "targets": [
          {
            "refId": "A",
            "query": "SELECT time_bucket('10 minute', timestamp) as time, AVG(timing.pageLoad) as avg_page_load FROM user-behavior WHERE eventType = 'performance' AND timestamp >= now() - INTERVAL '6 hours' GROUP BY time ORDER BY time",
            "datasource": "openobserve",
            "format": "time_series"
          },
          {
            "refId": "B",
            "query": "SELECT time_bucket('10 minute', timestamp) as time, AVG(timing.domInteractive) as avg_dom_interactive FROM user-behavior WHERE eventType = 'performance' AND timestamp >= now() - INTERVAL '6 hours' GROUP BY time ORDER BY time",
            "datasource": "openobserve",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
