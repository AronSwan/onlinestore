      {
        "id": "query-overview",
        "title": "查询概览",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "refId": "A",
            "query": "SELECT COUNT(*) as total_queries FROM query_log WHERE timestamp >= now() - INTERVAL '1 hour'",
            "datasource": "openobserve",
            "format": "time_series"
          },
          {
            "refId": "B",
            "query": "SELECT AVG(duration) as avg_query_duration FROM query_log WHERE timestamp >= now() - INTERVAL '1 hour'",
            "datasource": "openobserve",
            "format": "time_series"
          },
          {
            "refId": "C",
            "query": "SELECT COUNT(DISTINCT user_id) as active_users FROM query_log WHERE timestamp >= now() - INTERVAL '1 hour'",
            "datasource": "openobserve",
            "format": "time_series"
          },
          {
            "refId": "D",
            "query": "SELECT COUNT(*) as cached_queries FROM query_log WHERE cached = true AND timestamp >= now() - INTERVAL '1 hour'",
            "datasource": "openobserve",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "displayMode": "list",
          "orientation": "horizontal"
        }
      },
      {
        "id": "query-performance",
        "title": "查询性能分析",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "refId": "A",
            "query": "SELECT time_bucket('5 minute', timestamp) as time, AVG(duration) as avg_duration, percentile_cont(0.95) WITHIN GROUP (ORDER BY duration) as p95_duration FROM query_log WHERE timestamp >= now() - INTERVAL '1 hour' GROUP BY time ORDER BY time",
            "datasource": "openobserve",
            "format": "time_series"
          },
          {
            "refId": "B",
            "query": "SELECT time_bucket('5 minute', timestamp) as time, COUNT(*) as query_count FROM query_log WHERE timestamp >= now() - INTERVAL '1 hour' GROUP BY time ORDER BY time",
            "datasource": "openobserve",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              }
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        }
